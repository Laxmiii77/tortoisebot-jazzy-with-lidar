<?xml version="1.0" ?>
<sdf version="1.7">
  <model name="tortoisebot">
    <!-- Dummy root link with inertial, collisions, visuals -->
    <link name="dummy_root">
      <inertial>
        <pose>0.027273 0 0.013636 0 0 0</pose>
        <mass>1.1</mass>
        <inertia>
          <ixx>0.0171455</ixx>
          <ixy>0</ixy>
          <ixz>-0.00409091</ixz>
          <iyy>0.0478273</iyy>
          <iyz>0</iyz>
          <izz>0.0457818</izz>
        </inertia>
      </inertial>

      <!-- Base link collision and visual -->
      <collision name="dummy_root_fixed_joint_lump__base_link_collision">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.6 0.3 0.3</size>
          </box>
        </geometry>
      </collision>

      <visual name="dummy_root_fixed_joint_lump__base_link_visual">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.6 0.3 0.3</size>
          </box>
        </geometry>
        <material>
          <ambient>0.5 0.5 0.5 1</ambient>
          <diffuse>0.5 0.5 0.5 1</diffuse>
        </material>
      </visual>

      <!-- Lidar collision and visual -->
      <collision name="dummy_root_fixed_joint_lump__lidar_link_collision_1">
        <pose>0.3 0 0.15 0 0 0</pose>
        <geometry>
          <cylinder>
            <length>0.05</length>
            <radius>0.05</radius>
          </cylinder>
        </geometry>
        <surface>
          <contact>
            <ode/>
          </contact>
          <friction>
            <ode/>
          </friction>
        </surface>
      </collision>

      <visual name="dummy_root_fixed_joint_lump__lidar_link_visual_1">
        <pose>0.3 0 0.15 0 0 0</pose>
        <geometry>
          <cylinder>
            <length>0.01</length>
            <radius>0.02</radius>
          </cylinder>
        </geometry>
        <material>
          <ambient>0 1 0 1</ambient>
          <diffuse>0 1 0 1</diffuse>
        </material>
      </visual>

      <sensor name="lidar_sensor" type="gpu_lidar">
        <pose>0.3 0 0.15 0 0 0</pose>
        <topic>scan</topic>
        <update_rate>30</update_rate>
        <lidar>
          <scan>
            <horizontal>
              <samples>360</samples>
              <resolution>1</resolution>
              <min_angle>-0.2617</min_angle>
              <max_angle>0.2617</max_angle>
            </horizontal>
            <vertical>
              <samples>1</samples>
              <resolution>1</resolution>
              <min_angle>0</min_angle>
              <max_angle>0</max_angle>
            </vertical>
          </scan>
          <range>
            <min>0.12</min>
            <max>3.5</max>
            <resolution>0.01</resolution>
          </range>
          <noise>
            <type>gaussian</type>
            <mean>0</mean>
            <stddev>0.01</stddev>
          </noise>
        </lidar>
        <always_on>1</always_on>
        <visualize>1</visualize>
      </sensor>
    </link>

    
    <joint name="front_caster_joint" type="revolute">
      <pose relative_to="dummy_root">0.3 0 0 0 0 0</pose>
      <parent>dummy_root</parent>
      <child>front_caster</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-1e+16</lower>
          <upper>1e+16</upper>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
        </dynamics>
      </axis>
    </joint>

    <link name="front_caster">
      <pose relative_to="front_caster_joint">0 0 0 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.1</mass>
        <inertia>
          <ixx>0.00083</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.00083</iyy>
          <iyz>0</iyz>
          <izz>0.000167</izz>
        </inertia>
      </inertial>
      <collision name="front_caster_collision">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.1 0.1 0.3</size>
          </box>
        </geometry>
      </collision>
      <visual name="front_caster_visual">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.1 0.1 0.3</size>
          </box>
        </geometry>
        <material>
          <ambient>0.2 0.2 0.8 1</ambient>
          <diffuse>0.2 0.2 0.8 1</diffuse>
        </material>
      </visual>
    </link>

    <!-- Front wheel joint and link -->
    <joint name="front_wheel_joint" type="revolute">
      <pose relative_to="front_caster">0.05 0 -0.15 -1.5708 0 0</pose>
      <parent>front_caster</parent>
      <child>front_wheel</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-1e+16</lower>
          <upper>1e+16</upper>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
        </dynamics>
      </axis>
    </joint>

    <link name="front_wheel">
      <pose relative_to="front_wheel_joint">0 0 0 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.1</mass>
        <inertia>
          <ixx>5.1458e-05</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>5.1458e-05</iyy>
          <iyz>0</iyz>
          <izz>6.125e-05</izz>
        </inertia>
      </inertial>
      <collision name="front_wheel_collision">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <cylinder>
            <length>0.05</length>
            <radius>0.035</radius>
          </cylinder>
        </geometry>
      </collision>
      <visual name="front_wheel_visual">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <cylinder>
            <length>0.05</length>
            <radius>0.035</radius>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.1 0.1 0.1 1</ambient>
          <diffuse>0.1 0.1 0.1 1</diffuse>
        </material>
      </visual>
    </link>

    <!-- Left wheel joint and link -->
    <joint name="left_wheel_joint" type="revolute">
      <pose relative_to="dummy_root">-0.2825 0.125 -0.15 -1.5708 0 0</pose>
      <parent>dummy_root</parent>
      <child>left_wheel</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-1e+16</lower>
          <upper>1e+16</upper>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
        </dynamics>
      </axis>
    </joint>

    <link name="left_wheel">
      <pose relative_to="left_wheel_joint">0 0 0 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.1</mass>
        <inertia>
          <ixx>5.1458e-05</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>5.1458e-05</iyy>
          <iyz>0</iyz>
          <izz>6.125e-05</izz>
        </inertia>
      </inertial>
      <collision name="left_wheel_collision">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <cylinder>
            <length>0.05</length>
            <radius>0.035</radius>
          </cylinder>
        </geometry>
      </collision>
      <visual name="left_wheel_visual">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <cylinder>
            <length>0.05</length>
            <radius>0.035</radius>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.1 0.1 0.1 1</ambient>
          <diffuse>0.1 0.1 0.1 1</diffuse>
        </material>
      </visual>
    </link>

    <!-- Right wheel joint and link -->
    <joint name="right_wheel_joint" type="revolute">
      <pose relative_to="dummy_root">-0.2825 -0.125 -0.15 -1.5708 0 0</pose>
      <parent>dummy_root</parent>
      <child>right_wheel</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-1e+16</lower>
          <upper>1e+16</upper>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
        </dynamics>
      </axis>
    </joint>

    <link name="right_wheel">
      <pose relative_to="right_wheel_joint">0 0 0 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.1</mass>
        <inertia>
          <ixx>5.1458e-05</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>5.1458e-05</iyy>
          <iyz>0</iyz>
          <izz>6.125e-05</izz>
        </inertia>
      </inertial>
      <collision name="right_wheel_collision">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <cylinder>
            <length>0.05</length>
            <radius>0.035</radius>
          </cylinder>
        </geometry>
      </collision>
      <visual name="right_wheel_visual">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <cylinder>
            <length>0.05</length>
            <radius>0.035</radius>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.1 0.1 0.1 1</ambient>
          <diffuse>0.1 0.1 0.1 1</diffuse>
        </material>
      </visual>
    </link>

    <static>0</static>

    <!-- Differential drive plugin for Gazebo Harmonic -->
    <plugin filename="gz-sim-diff-drive-system" name="gz::sim::systems::DiffDrive">
      <left_joint>left_wheel_joint</left_joint>
      <right_joint>right_wheel_joint</right_joint>
      <wheel_separation>0.25</wheel_separation>
      <wheel_radius>0.035</wheel_radius>
      <odom_publish_frequency>50</odom_publish_frequency>
      <topic>cmd_vel</topic>
      <odom_topic>odom</odom_topic>
      <frame_id>odom</frame_id>
      <child_frame_id>dummy_root</child_frame_id>
    </plugin>

    <!-- Joint State Publisher Plugin -->
    <plugin filename="gz-sim-joint-state-publisher-system" name="gz::sim::systems::JointStatePublisher">
      <topic>joint_states</topic>
    </plugin>
    
    <!-- FIXED: ROS2 Laser Scan Plugin with correct sensor name and frame -->
    <plugin filename="gz-sim-sensors-system" name="gz::sim::systems::Sensors">
      <render_engine>ogre2</render_engine>
    </plugin>

  </model>
</sdf>